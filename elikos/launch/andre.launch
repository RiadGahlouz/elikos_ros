<launch>
	<include file="$(find elikos_ros)/launch/px4.launch" />

	<node pkg="elikos_ros" type="pose_offset" name="pose_offset" />

	  <node pkg="camera1394" type="camera1394_node" name="camera1394">
    <param name="guid" value="00b09d0100db6e40" />
    <param name="video_mode" value="640x480_mono8" />
    <param name="frame_rate" value="60" />
    <!-- color stuff -->
    <param name="bayer_pattern" value="rggb" />
    <param name="bayer_method" value="" />

  </node>

  <node pkg="camera1394" type="camera1394_node" name="camera1394_2">
    <param name="guid" value="00b09d01008f706f" />
    <param name="video_mode" value="640x480_mono8" />
    <param name="frame_rate" value="60" />

    <remap from="camera" to="camera_2" />
  </node>

</launch>